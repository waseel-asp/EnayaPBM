package com.enayapbm.medimpact.model;

import java.util.Date;

public class MedImpactTransactionWrapper {

	private MedImpactRequest request;
	private MedImpactResponse response;
	private String pbmRequestId;
	private String pbmTransactionLogId;
	private Date dateTime;
	private RequestType requestType;
	private MedImpactCancellationRequest cancellationRequest;
	private MedImpactCancellationResponse cancellationResponse;
	private String portalTransactionId;
	private String referenceNumber;

	public enum RequestType {
		NEW, FOLLOWUP, CANCELLATION
	};

	public MedImpactTransactionWrapper() {
	}

	public MedImpactRequest getRequest() {
		return request;
	}

	public void setRequest(MedImpactRequest request) {
		this.request = request;
	}

	public MedImpactResponse getResponse() {
		return response;
	}

	public void setResponse(MedImpactResponse response) {
		this.response = response;
	}

	public String getPbmRequestId() {
		return pbmRequestId;
	}

	public void setPbmRequestId(String pbmRequestId) {
		this.pbmRequestId = pbmRequestId;
	}

	public String getPbmTransactionLogId() {
		return pbmTransactionLogId;
	}

	public void setPbmTransactionLogId(String pbmTransactionLogId) {
		this.pbmTransactionLogId = pbmTransactionLogId;
	}

	public Date getDateTime() {
		return dateTime;
	}

	public void setDateTime(Date dateTime) {
		this.dateTime = dateTime;
	}

	public RequestType getRequestType() {
		return requestType;
	}

	public void setRequestType(RequestType requestType) {
		this.requestType = requestType;
	}

	public MedImpactCancellationRequest getCancellationRequest() {
		return cancellationRequest;
	}

	public void setCancellationRequest(MedImpactCancellationRequest cancellationRequest) {
		this.cancellationRequest = cancellationRequest;
	}

	public MedImpactCancellationResponse getCancellationResponse() {
		return cancellationResponse;
	}

	public void setCancellationResponse(MedImpactCancellationResponse cancellationResponse) {
		this.cancellationResponse = cancellationResponse;
	}

	public String getPortalTransactionId() {
		return portalTransactionId;
	}

	public void setPortalTransactionId(String portalTransactionId) {
		this.portalTransactionId = portalTransactionId;
	}

	public String getReferenceNumber() {
		return referenceNumber;
	}

	public void setReferenceNumber(String referenceNumber) {
		this.referenceNumber = referenceNumber;
	}

}
